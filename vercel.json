{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ws",
      "dest": "/api/ws.js"
    },
    {
      "src": "/icon.png",
      "dest": "/public/icon.png"
    },
    {
      "src": "/preview.png",
      "dest": "/public/preview.png"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 